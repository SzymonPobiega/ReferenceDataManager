@model ReferenceDataManager.Sample.Web.Models.ChangeSetTreeViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Header
{
    <link href="@Url.Content("~/Content/treeview/jquery.treeview.css")" rel="stylesheet" type="text/css" />
}
@section Footer
{
    <script src="@Url.Content("~/Scripts/jquery.treeview.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#tree").treeview({
                collapsed: true,
                animated: "fast"
            });
        });
    </script>
}
<h2>
    Change set tree</h2>
<ul id="tree">
    @foreach (var root in Model.RootNodes)
    {
        Html.RenderPartial("_Node", root);
    }
</ul>
